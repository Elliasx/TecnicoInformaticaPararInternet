programa
{
	
	funcao inicio()
	{
		inteiro numero1,numero2

		escreva("Diga os dois valores: \n")
		leia(numero1,numero2)
		limpa()

		se(numero1 > numero2) {
				escreva("O ",numero1," é maior que o ",numero2)
			} senao {
				escreva("O ",numero2," é maior que o ",numero1)
				}
	}
}
